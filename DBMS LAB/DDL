DROP DATABASE IF EXISTS STUDENT;
CREATE SCHEMA STUDENT;
USE STUDENT;
CREATE TABLE student(sid INT,name VARCHAR(20),gender VARCHAR(20),address VARCHAR(20),age INT);
INSERT INTO student VALUES(345,'SONY','female','hyderabad',18);
INSERT INTO student VALUES(346,'pinky','female','hyderabad',19);
INSERT INTO student VALUES(400,'ninny','female','mahabuubnagar',20);
SELECT * FROM student;
DELETE FROM student s WHERE s.name='sony';
SELECT * FROM student;
ALTER TABLE student ADD COLUMN salary INT;
SELECT * FROM student;
